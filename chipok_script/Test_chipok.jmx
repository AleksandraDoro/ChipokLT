          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.http.client.methods.HttpGet
import org.apache.http.impl.client.HttpClients
import org.apache.http.util.EntityUtils

def name = vars.get(&quot;item&quot;).replaceAll(&quot; &quot;, &quot;%20&quot;)

// URL для запроса
def url = &quot;http://localhost:8080/api/item/${name}&quot;

// URL для отладки
log.info(url)

def request = new HttpGet(url)

// Настройка HTTP клиента
def httpClient = HttpClients.createDefault()

try {
    // Выполняем запрос, получаем ответ
    def response = httpClient.execute(request)

    // Извлекаем тело ответа
    def responseBody = EntityUtils.toString(response.getEntity())

    // Выводим тело ответа в лог
    log.info(&quot;Response: ${responseBody}&quot;)
SampleResult.setResponseData(responseBody,&quot;UTF-8&quot;)
    // Закрываем ресурсы
    response.close()
} finally {
    // Закрываем клиент
    httpClient.close()
}</stringProp>
          </JSR223Sampler>
